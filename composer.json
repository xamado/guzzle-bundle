{
    "name": "xamado/guzzle-bundle",
    "type": "symfony-bundle",
    "description": "Guzzle bundle to use with Symfony 2",
    "keywords": ["guzzle", "bundle"],
    "homepage": "http://github.com/Xamado/GuzzleBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Xavier Amado",
            "email": "xamado@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "guzzle/guzzle":"3.0.*",
        "jms/serializer-bundle": "1.0.*"
    },
    "require-dev": {
        "symfony/framework-bundle": ">=2.1,<2.2-dev"
    },
    "autoload": {
        "psr-0": { "Xamado\\GuzzleBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Xamado/GuzzleBundle"
}
